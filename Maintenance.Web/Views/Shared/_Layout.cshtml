@{
    string controller = "";
    var controllerFromUrl = ViewContext.RouteData.Values["Controller"];
    if (controllerFromUrl != null)
    {
        controller = controllerFromUrl.ToString();
    }

    string action = "";
    var actionFromUrl = ViewContext.RouteData.Values["action"];
    if (actionFromUrl != null)
    {
        action = actionFromUrl.ToString();
    }

    string Lang = System.Globalization.CultureInfo.CurrentCulture.Name;
    var isArabic = Lang == "ar-EG";

    var sideMenuHelper = new SideMenuHelper(controller, action);

    var userType = "";
    switch (ViewBag.UserType)
    {
        case "Administrator":
            userType = Messages.Administrator;
            break;
        case "MaintenanceManager":
            userType = Messages.MaintenanceManager;
            break;
        case "MaintenanceTechnician":
            userType = Messages.MaintenanceTechnician;
            break;
    }
}

